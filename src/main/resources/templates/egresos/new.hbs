{{#partial "contenido"}}

    <form action="/egresos" method="POST">

        <div class="form-group">
            <label for="nombre">Nombre</label>
            <input type="text" placeholder="Nombre del Egreso" name="nombre">
        </div>

        <div class="form-group">
            <label for="id">Id </label>
            <input type="number" placeholder="Id del egreso" name="id">
        </div>

        <input class="btn btn-primary btn-lg" type="submit" value="Guardar">
    </form>
{{/partial}}

{{>layout.hbs}}